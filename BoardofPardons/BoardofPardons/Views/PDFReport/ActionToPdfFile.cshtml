                            {
                                <text>Yes</text>
                            }
                            else
                            {
                                <text>No</text>
                            }
                            

                        </div>
                        <div class="col-lg-1">
                            Date Granted
                        </div>
                        <div class="col-lg-4">
                            @Html.DisplayFor(model => model.IncarceratedStep1.DateOtherStateGranted)
