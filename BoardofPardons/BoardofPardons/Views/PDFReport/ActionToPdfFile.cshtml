@model BoardofPardons.Entity.AllFormReview
@{
    ViewBag.Title = "review";
}
@if (Model != null && Model.type == 2)
{
    if (Model.IncarceratedStep1 != null)
    {
            <div class="container">
        <div class="well">
            <div class="text-center">
                <h4>Pennsylvania Board of Pardons Commonwealth of Pennsylvania Application for Clemency</h4>
            </div>
            <p><strong>Important:</strong>This application should be completed by non-incarcerated applicants only. All questions must be answered. Please answer "N/A" for questions that are not applicable to you. </p>
            <div class="row" id="formContent">
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Official Use Only: Do not complete this section.</div>
                <div class="panel-body">
                    <div class="col-lg-12">
                        <div class="col-lg-4">
                            Aplication Number:@Html.DisplayFor(model => model.IncarceratedStep1.AplicationNumber)
                        </div>
                        <div class="col-lg-4">
                            Board of Pardon Number:@Html.DisplayFor(model => model.IncarceratedStep1.PardonNumber)
                        </div>
                        <div class="col-lg-4">
                            Session Date:@Html.DisplayFor(model => model.IncarceratedStep1.SessionDate)
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">SECTION 1: Type of Clemency Requested</div>
                <div class="panel-body">
                    <div class="col-lg-12">
                        <div class="col-lg-3">
                            @Html.DisplayFor(model => model.IncarceratedStep1.ClemencyRequested)
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12">
                        <div class="col-lg-5">
                            Sentence :@Html.DisplayFor(model => model.IncarceratedStep1.Sentence)
                        </div>
                        <div class="col-lg-3">
                            Minimum Date:@Html.DisplayFor(model => model.IncarceratedStep1.MinimumDate)
                        </div>
                        <div class="col-lg-3">
                            Maximum Date:@Html.DisplayFor(model => model.IncarceratedStep1.MaximumDate)
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12">
                        <div class="col-lg-9">
                            <p class="lead">Have you previously filed a pardon application in Pennsylvania?</p>
                        </div>
                        <div class="col-lg-2">
                            @if (Model.IncarceratedStep1.HaveFiled)
                            {
                                <text>Yes</text>
                            }
                            else
                            {
                                <text>No</text>
                            }
                        </div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <div class="form-group">
                        <p class="col-lg-12 help-block">If you answered YES, list date(s) of your prevous application(s) and application numbers (if known, (if the answer is no, put N/A):</p>
                        <div class="col-lg-12 form-group">
                            
                            @Html.DisplayFor(model => model.IncarceratedStep1.Previous)
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
                <hr />
                <div class="col-lg-12">
                    <div class="col-lg-9">
                        <p class="lead">Have you previously filed a pardon application in another state?</p>
                    </div>
                    <div class="col-lg-2">
                        @if (Model.IncarceratedStep1.OtherState)
                        {
                            <text>Yes</text>
                        }
                        else
                        {
                            <text>No</text>
                        }
