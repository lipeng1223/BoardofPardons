                    </div>
                    <div class="col-lg-3 form-group">
                        <label>
                            Place of Birth:
                        </label>
                        @Html.DisplayFor(model => model.IncarceratedStep1.BirthPlace)
                    </div>
                    <div class="col-lg-3 form-group">
                        <label class="">
                            Gender:@Html.DisplayFor(model => model.IncarceratedStep1.Gender)
                        </label>
                    </div>
                    <div class="col-lg-3 form-group">
                        <label>
                            SS Number:@Html.DisplayFor(model => model.IncarceratedStep1.SSN)
                        </label>
                    </div>
                </div>
                <div class="clearfix"></div>
