                    @Html.DisplayFor(model => model.IncarceratedStep1.InstName)
                </div>
                <div class="col-lg-4">
                    Inmate Number Number:@Html.DisplayFor(model => model.IncarceratedStep1.InmateNumber)
                </div>
                <div class="clearfix"></div>
                <div class="col-lg-8">
                    Institution Address:@Html.DisplayFor(model => model.IncarceratedStep1.InstAddress)
                </div>
                <div class="col-lg-4">
                    Parole Number:@Html.DisplayFor(model => model.IncarceratedStep1.paroleNumber)
                </div>
                <div class="clearfix"></div>
                <div class="col-lg-5 form-group">
                    City:@Html.DisplayFor(model => model.IncarceratedStep1.ApplicantCity)
                </div>
                <div class="col-lg-3 form-group">
                    State:@Html.DisplayFor(model => model.IncarceratedStep1.ApplicantState)
                </div>
                <div class="col-lg-4 form-group">
                    Zip:@Html.DisplayFor(model => model.IncarceratedStep1.ApplicantZip)
