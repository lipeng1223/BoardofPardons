                        </div>
                        <div class="col-lg-12">
                            <div class="form-inline form-group">
                                <label>If you answered no, state outstanding balance</label>
                                @Html.DisplayFor(model => model.IncarceratedStep2.FinesBalance)
                                <label>Date of last payment</label>
                                @Html.DisplayFor(model => model.IncarceratedStep2.LastPaymentDate)
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <hr />
                        <div class="col-lg-6">
                            <p class="lead">Have you ever violated probation?</p>
                        </div>
                        <div class="col-lg-6">
                            @if (Model.IncarceratedStep2.ViolatedParole != null && Model.IncarceratedStep2.ViolatedParole == true)
                            {
                                <text>Yes</text>
                            }
