@model BoardofPardons.Entity.AllFormReview
@{
    ViewBag.Title = "review";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .kbw-signature {
        height: 200px;
    }
</style>

@if (Model != null && Model.type == 2)
{
    <div class="container">
        <div class="well">
            <div class="text-center">
                <h4>Pennsylvania Board of Pardons Commonwealth of Pennsylvania Application for Clemency</h4>
            </div>
            <p><strong>Important:</strong>This application should be completed by non-incarcerated applicants only. All questions must be answered. Please answer "N/A" for questions that are not applicable to you. </p>
            <div class="row" id="formContent">
            </div>

            <div class="panel panel-default">
                <div class="panel-heading">Official Use Only: Do not complete this section.</div>
                <div class="panel-body">
                    <div class="col-lg-12">

                        <div class="col-lg-4">
                            <div class=" ">
                                <label>
                                    Aplication Number
                                </label>

                                @Html.TextBoxFor(model => model.IncarceratedStep1.AplicationNumber, new { @class = "form-control", id = "AplicationNumber" })
                            </div>

                        </div>
                        <div class="col-lg-4">
                            <label>
                                Board of Pardon Number
                            </label>

                            @Html.TextBoxFor(model => model.IncarceratedStep1.PardonNumber, new { @class = "form-control", id = "PardonNumber" })
                        </div>
                        <div class="col-lg-4">
                            <label>
                                Session Date
                            </label>

                            @Html.TextBoxFor(model => model.IncarceratedStep1.SessionDate, new { @class = "form-control", id = "SessionDate" })
                        </div>

                    </div>
                    <div class="clearfix"></div>
                    <hr />
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">SECTION 1: Type of Clemency Requested</div>
                <div class="panel-body">
                    <div class="col-lg-12">

                        <div class="col-lg-3">
                            <div class=" ">
                                <label>

                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.ClemencyRequested, "Commute Death Sentence to life inmprisonment", new { @checked = "checked" })
                                    Commute Death Sentence
                                    to life inmprisonment
                                </label>
                            </div>

                        </div>
                        <div class="col-lg-3">
                            <label>

                                @Html.RadioButtonFor(model => model.IncarceratedStep1.ClemencyRequested, "Commute Life Sentence to life on Parole")
                                Commute Life Sentence to life on Parole
                            </label>
                        </div>
                        <div class="col-lg-2">
                            <label>

                                @Html.RadioButtonFor(model => model.IncarceratedStep1.ClemencyRequested, "Commute Minimum Sentence")
                                Commute Minimum Sentence
                            </label>
                        </div>
                        <div class="col-lg-2">
                            <label>

                                @Html.RadioButtonFor(model => model.IncarceratedStep1.ClemencyRequested, "Commute Maximum Sentence")
                                Commute Maximum Sentence
                            </label>
                        </div>
                        <div class="col-lg-2">
                            <label>

                                @Html.RadioButtonFor(model => model.IncarceratedStep1.ClemencyRequested, "Pardon")
                                Pardon
                            </label>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12">

                        <div class="col-lg-5">
                            <p>Sentence</p>
                            <div class="radio-inline">
                                <label>

                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.Sentence, "Death", new { @checked = "checked" })
                                    Death
                                </label>
                            </div>
                            <div class="radio-inline">
                                <label>

                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.Sentence, "Life")
                                    Life
                                </label>
                            </div>
                            <div class="radio-inline">
                                <label>

                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.Sentence, "Min/Max")
                                    Min/Max
                                </label>
                            </div>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-inline">
                                <label>Minimum Date</label>
                                @*<input type="date" class="form-control" name="MinimumDate" />*@
                                @Html.TextBoxFor(model => model.IncarceratedStep1.MinimumDate, new { type = "date", @class = "form-control", id = "MinimumDate" })
                            </div>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-inline">
                                <label>Maximum Date</label>
                                @*<input type="date" class="form-control" name="MaximumDate" />*@
                                @Html.TextBoxFor(model => model.IncarceratedStep1.MaximumDate, new { type = "date", @class = "form-control", id = "MaximumDate" })
                            </div>
                        </div>

                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12">
                        <div class="col-lg-9">
                            <p class="lead">Have you previously filed a pardon application in Pennsylvania?</p>
                        </div>
                        <div class="col-lg-2">
                            <div class="radio-inline">
                                <label>
                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.HaveFiled, "True", new { id = "otherstateFilledYes", @checked = "checked" })
                                    @*<input type="radio" name="havefiled" id="otherstateFilledYes" checked="checked">*@
                                    Yes
                                </label>
                            </div>
                            <div class="radio-inline">
                                <label>
                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.HaveFiled, "False", new { id = "otherstateFilledNo" })
                                    @*<input type="radio" name="havefiled" id="otherstateFilledNo" checked="">*@
                                    No
                                </label>
                            </div>
                        </div>

                    </div>
                    <div class="col-lg-12">
                        <div class="form-group">
                            <p class="col-lg-12 help-block">If you answered YES, list date(s) of your prevous application(s) and application numbers (if known, (if the answer is no, put N/A):</p>
                            <div class="col-lg-12 form-group">
                                @*<input type="text" id="txtprevious" name="previous" required class="form-control" />*@
                                @Html.TextBoxFor(model => model.IncarceratedStep1.Previous, new { @class = "form-control", id = "previous" })
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12">
                        <div class="col-lg-9">
                            <p class="lead">Have you previously filed a pardon application in another state?</p>
                        </div>
                        <div class="col-lg-2">
                            <div class="radio-inline">
                                <label>
                                    @*<input type="radio" name="OtherState" id="rbtnotheryes" checked="checked">*@
                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.OtherState, "True", new { id = "rbtnotheryes", @checked = "checked" })
                                    Yes
                                </label>
                            </div>
                            <div class="radio-inline">
                                <label>
                                    @*<input type="radio" name="OtherState" id="rbtnotherno" checked="">*@
                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.OtherState, "False", new { id = "rbtnotherno" })
                                    No
                                </label>
                            </div>
                        </div>

                    </div>
                    <div class="col-lg-12">
                        <div class="form-group">
                            <p class="col-lg-12 help-block">If you answered YES, provide the following:</p>
                            <div class="col-lg-2">Where Filed?</div>
                            <div class="col-lg-12 form-group">
                                @Html.TextBoxFor(model => model.IncarceratedStep1.WhereFiled, new { @class = "form-control", id = "WhereFiled" })
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="form-group">
                            <div class="col-lg-4">
                                Was it Granted?
                                <div class="radio-inline">
                                    <label>
                                        @*<input type="radio" name="GrantedOtherState" id="rbtnGrantedYes" checked="checked">*@
                                        @Html.RadioButtonFor(model => model.IncarceratedStep1.GrantedOtherState, "True", new { id = "GrantedOtherState", @checked = "checked" })
                                        Yes
                                    </label>
                                </div>
                                <div class="radio-inline">
                                    <label>
                                        @*<input type="radio" name="GrantedOtherState" id="rbtnGrantedNo" checked="">*@
                                        @Html.RadioButtonFor(model => model.IncarceratedStep1.GrantedOtherState, "False", new { id = "GrantedOtherState" })
                                        No
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-1">
                                Date Granted
                            </div>
                            <div class="col-lg-4">

                                @*<input type="date" id="txtdateotherstategranted" name="DateOtherStateGranted" class="form-control  form-group">*@
                                @Html.TextBoxFor(model => model.IncarceratedStep1.DateOtherStateGranted, new { type = "date", @class = "form-control", id = "txtdateotherstategranted" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">SECTION 2: Applicant Information</div>
                <div class="panel-body">

                    <div class="col-lg-12">

                        <div class="col-lg-3">
                            <div class="form-group ">
                                <label>
                                    Last Name
                                </label>
                                @*<input type="text" class="form-control LastNameValidate" id="txtlastname" name="LastName" />*@
                                @Html.TextBoxFor(model => model.IncarceratedStep1.LastName, new { @class = "form-control LastName", id = "txtlastname" })
                            </div>

                        </div>
                        <div class="col-lg-3 form-group">
                            <label>
                                First Name
                            </label>
                            @*<input type="text" class="form-control FirstNameValidate" id="txtfirstname" name="FirstName" />*@
                            @Html.TextBoxFor(model => model.IncarceratedStep1.FirstName, new { @class = "form-control FirstName", id = "txtfirstname" })
                        </div>
                        <div class="col-lg-3 form-group">
                            <label>
                                Middle Name
                            </label>
                            @*<input type="text" class="form-control txtmiddlename" id="txtmiddlename" name="MiddleName" />*@
                            @Html.TextBoxFor(model => model.IncarceratedStep1.MiddleName, new { @class = " MiddleName form-control" })
                        </div>
                        <div class="col-lg-3 form-group">
                            <label>
                                Suffix
                            </label>
                            @*<input type="text" class="form-control" id="txtsuffix" name="Suffix" />*@
                            @Html.TextBoxFor(model => model.IncarceratedStep1.Suffix, new { @class = "form-control" })
                        </div>


                    </div>
                    <div class="col-lg-12">

                        <div class="col-lg-3 form-group">
                            <label>
                                Birth Date
                            </label>
                            @*<input type="date" id="txtbirthdate" name="BirthDate" class="form-control dateRequired dobRangeCheck" />*@
                            @Html.TextBoxFor(model => model.IncarceratedStep1.BirthDate, new { type = "date", @class = "form-control dateRequired dobRangeCheck" })
                        </div>
                        <div class="col-lg-3 form-group">
                            <label>
                                Place of Birth
                            </label>
                            @*<input type="text" class="form-control" id="txtbirthplace" name="BirthPlace" />*@
                            @Html.TextBoxFor(model => model.IncarceratedStep1.BirthPlace, new { @class = "form-control" })
                        </div>
                        <div class="col-lg-3 form-group">
                            <label class="">
                                Gender
                            </label><br />
                            <div class="radio-inline">
                                <label>
                                    @*<input type="radio" name="gender" id="rbtnPrevAppYes" checked="checked">
                                        Male*@
                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.Gender, "Male", new { id = "male", @checked = "checked" })
                                    Male
                                </label>
                            </div>
                            <div class="radio-inline">
                                <label>
                                    @Html.RadioButtonFor(model => model.IncarceratedStep1.Gender, "Female", new { id = "female" })
                                    Female
                                </label>
                            </div>
                        </div>
                        <div class="col-lg-3 form-group">
                            <label>
                                SS Number
                            </label>
                            @Html.TextBoxFor(model => model.IncarceratedStep1.SSN, new { @class = "form-control SSNValidate" })
                        </div>


                    </div>
                    <div class="clearfix"></div>
                    <hr />

                    <div class="col-lg-12">
                        <h5>Race: (for statistical purposes only)</h5>
                        <br />
                        <div class="radio-inline">
                            <label>
                                @Html.RadioButtonFor(model => model.IncarceratedStep1.Race, "White/Caucasian", new { @checked = "checked" })
                                @*<input type="radio" value="White/Caucasian" name="race" id="rbtnPrevAppYes" checked="checked">*@
                                White/Caucasian
                            </label>
                        </div>
                        <div class="radio-inline">
                            <label>
                                @*<input type="radio" value="Black/African American" name="race" id="rbtnPrevAppNo" checked="">*@
                                @Html.RadioButtonFor(model => model.IncarceratedStep1.Race, "Black/African American")
                                Black/African American
                            </label>
                        </div>
                        <div class="radio-inline">
                            <label>
                                @*<input type="radio" value="Asian" name="race" id="rbtnPrevAppYes" checked="">*@
                                @Html.RadioButtonFor(model => model.IncarceratedStep1.Race, "Asian")
                                Asian
                            </label>
                        </div>
                        <div class="radio-inline">
                            <label>
                                @*<input type="radio" value="Native American" name="race" id="rbtnPrevAppNo" checked="">*@
                                @Html.RadioButtonFor(model => model.IncarceratedStep1.Race, "Native American")
                                Native American
                            </label>
                        </div>
                        <div class="radio-inline">
                            <label>
                                @*<input type="radio" value="Hispanic/Latino" name="race" id="rbtnPrevAppYes" checked="">*@
                                @Html.RadioButtonFor(model => model.IncarceratedStep1.Race, "Hispanic/Latino")
                                Hispanic/Latino
                            </label>
                        </div>
                        <div class="radio-inline">
                            <label>
                                @*<input type="radio" value="Other" name="race" id="rbtnPrevAppNo" checked="">*@
                                @Html.RadioButtonFor(model => model.IncarceratedStep1.Race, "Other")
                                Other
                            </label>
                        </div>
                        <div class="radio-inline">
                            <label>
                                @*<input type="radio" value="Do not wish to disclose" name="race" id="rbtnPrevAppYes" checked="">*@
                                @Html.RadioButtonFor(model => model.IncarceratedStep1.Race, "Do not wish to disclose")
                                Do not wish to disclose
                            </label>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-8">
                        <label>Institution Name</label>

                        @Html.TextBoxFor(model => model.IncarceratedStep1.InstName, new { @class = "form-control", id = "previous" })
                    </div>
                    <div class="col-lg-4">
                        <label>Inmate Number Number</label>

                        @Html.TextBoxFor(model => model.IncarceratedStep1.InmateNumber, new { @class = "form-control", id = "InmateNumber" })
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-lg-8">
                        <label>Institution Address</label>

                        @Html.TextBoxFor(model => model.IncarceratedStep1.InstAddress, new { @class = "form-control", id = "InstAddress" })
                    </div>
                    <div class="col-lg-4">
                        <label>Parole Number</label>
                        @Html.TextBoxFor(model => model.IncarceratedStep1.paroleNumber, new { @class = "form-control", id = "paroleNumber" })
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-lg-5 form-group">
                        <label>City</label>
                        @Html.TextBoxFor(model => model.IncarceratedStep1.ApplicantCity, new { @class = "form-control", id = "City" })
                    </div>
                    <div class="col-lg-3 form-group">
                        <label>State</label>
                        @Html.TextBoxFor(model => model.IncarceratedStep1.ApplicantState, new { @class = "form-control" })
                    </div>
                    <div class="col-lg-4 form-group">
                        <label>Zip</label>
                        @Html.TextBoxFor(model => model.IncarceratedStep1.ApplicantZip, new { @class = "form-control" })
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12 form-group">
                        <label>Aliases and Other Names: State every other name by which you have been known. Include your maiden name, names by former marriages, aliases and nicknames.</label>
                        @Html.TextBoxFor(model => model.IncarceratedStep1.Aliases, new { @class = "form-control " })
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12">
                        <div class="col-lg-6">
                            <p>
                                Representation:  Who will represent if you are granted a public hearing?<br />
                                NOTE: Incarcerated Applicants MAY NOT represent themselves.<br />
                                If you do not have representation, please contact:<br />
                                The Interagency Laison<br />
                                Bureau of Treatment Services<br />
                                Pennsylvania Department of Corrections<br />
                                1920 Technology Parkway<br />
                                Mechanicsburg, PA 17050<br />
                                (717)728-0380
                            </p>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label>Title</label>
                                @Html.TextBoxFor(model => model.IncarceratedStep1.Title, new { @class = "form-control " })
                            </div>
                            <div class="form-group">
                                <label> Address</label>
                                @Html.TextBoxFor(model => model.IncarceratedStep1.Address, new { @class = "form-control " })
                            </div>
                            <div class="form-group">
                                <label>address2: (appartment, floor, suite, etc.)</label>
                                @Html.TextBoxFor(model => model.IncarceratedStep1.Address2, new { @class = "form-control " })
                            </div>
                            <div class="form-group">
                                <label>City, State, Zip </label>
                                @Html.TextBoxFor(model => model.IncarceratedStep1.CityStateZip, new { @class = "form-control" })
                            </div>
                            <div class="form-group">
                                <label>Phone</label>
                                @Html.TextBoxFor(model => model.IncarceratedStep1.Phone, new { @class = "form-control " })
                            </div>
                        </div>
                    </div>

                </div>
            </div>


        </div>
    </div>
    <div class="container">
        <div class="well">
            <div class="text-center">
                <h4>Pennsylvania Board of Pardons Commonwealth of Pennsylvania Application for Clemency</h4>
            </div>
            <p><strong>Important:</strong>This application should be completed by non-incarcerated applicants only. All questions must be answered. Please answer "N/A" for questions that are not applicable to you. </p>
            <div class="row" id="formContent">
            </div>

            <div class="panel panel-default">
                <div class="panel-heading">
                    <h5> SECTION 3: Convictions For Which Clemency Requested</h5>
                    <h6>Important: Only convictions referenced in section 3 will be considered for clemency. See filing instruction #9 for information regarding required attachments.</h6>

                </div>
                <div class="panel-body">
                    <div class="col-lg-12">
                        <div class="col-lg-1">
                            <b>Your Case #1</b>
                        </div>
                        <div class="col-lg-3 form-group">
                            <div class=" ">
                                <label>
                                    Date of Incident
                                </label>

                                @Html.TextBoxFor(model => model.IncarceratedStep2.DateINC, new { type = "date", @class = "form-control", id = "DateINC" })
                            </div>

                        </div>
                        <div class="col-lg-3 form-group">
                            <label>
                                County
                            </label>

                            @Html.TextBoxFor(model => model.IncarceratedStep2.ConvictCounty, new { @class = "form-control", id = "ConvictCounty" })
                        </div>
                        <div class="col-lg-2 form-group">
                            <label>
                                OTN#
                            </label>

                            @Html.TextBoxFor(model => model.IncarceratedStep2.ConvictOTN, new { @class = "form-control", id = "ConvictOTN" })
                        </div>
                        <div class="col-lg-3 form-group">
                            <label>
                                Docket Number:
                            </label>

                            @Html.TextBoxFor(model => model.IncarceratedStep2.ConvictDocket, new { type = "date", @class = "form-control", id = "ConvictDocket" })
                        </div>


                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12">
                        <div class="col-lg-4">
                            <div class="radio">
                                <label>

                                    @Html.RadioButtonFor(model => model.IncarceratedStep2.Guilty, "Found Guilty", new { @checked = "checked" })
                                    Found Guilty
                                </label>
                            </div>
                            <div class="radio">
                                <label>

                                    @Html.RadioButtonFor(model => model.IncarceratedStep2.Guilty, "Pled Guilty", new { @checked = "checked" })
                                    Pled Guilty
                                </label>
                            </div>
                            <div class="radio">
                                <label>

                                    @Html.RadioButtonFor(model => model.IncarceratedStep2.Guilty, "Pled Nolo Contendre (no contest)", new { @checked = "checked" })
                                    Pled Nolo Contendre (no contest)
                                </label>
                            </div>

                        </div>
                        <div class="col-lg-4 form-group">
                            <label>
                                Date of Sentence
                            </label>

                            @Html.TextBoxFor(model => model.IncarceratedStep2.DateSentence, new { @class = "form-control", id = "DateSentence" })
                        </div>
                        <div class="col-lg-4 form-group">
                            <label>
                                Judge
                            </label>

                            @Html.TextBoxFor(model => model.IncarceratedStep2.ConvictJudge, new { @class = "form-control", id = "ConvictJudge" })
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <hr />

                    <div class="col-lg-12">
                        <p>State the Offense(s) and Sentence(s) for this case. Sentences should include any ordered fines, costs and restitution: (list each offense and sentence on seperate lines)</p>
                    </div>
                    <div class="col-lg-12">

                        <div class="col-lg-6 form-group">
                            <label><strong>Offense(s)</strong> </label>

                            @Html.TextAreaFor(model => model.IncarceratedStep2.Offenses, new { @class = "form-control", id = "Offenses" })
                        </div>
                        <div class="col-lg-6 form-group">
                            <label><strong>Sentence(s)</strong> </label>

                            @Html.TextAreaFor(model => model.IncarceratedStep2.Sentence, new { @class = "form-control", id = "Sentence" })
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12">
                        <p>Provide the offenses that were nolle prossed, withdrawn, dismissed, dropped or found not guilty: (enter N/A if this does not apply)</p>
                    </div>
                    <div class="col-lg-7 form-group">

                        @Html.TextAreaFor(model => model.IncarceratedStep2.NotGuilty, new { @class = "form-control", id = "NotGuilty" })
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-12">
                        <p>In your own words, provide detailed facts of incident, which MUST include how you were involved:</p>
                    </div>
                    <div class="col-lg-7 form-group">

                        @Html.TextAreaFor(model => model.IncarceratedStep2.Facts, new { @class = "form-control", id = "Facts" })
                    </div>
                    <div class="clearfix"></div>
                    <hr />
                    <div class="col-lg-6">
                        <p class="lead">Have you paid fines and restitution?</p>
                    </div>
                    <div class="col-lg-6">
                        <div class="radio-inline">
                            <label>

                                @Html.RadioButtonFor(model => model.IncarceratedStep2.HavePaid, "True", new { @checked = "checked" })
                                Yes
                            </label>
                        </div>
                        <div class="radio-inline">
                            <label>

                                @Html.RadioButtonFor(model => model.IncarceratedStep2.HavePaid, "False", new { @checked = "checked" })
                                No
                            </label>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <p>(If you answered yes, provide a statement from the Clerk of Courts office that indicates a zero balance) See filing instructions #9</p>
                    </div>
                    <div class="col-lg-12">
