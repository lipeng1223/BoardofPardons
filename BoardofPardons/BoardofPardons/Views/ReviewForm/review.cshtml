<div class="container">
    <div class="well">
        <div class="row">

            <div class="panel panel-default">
                <div class="panel-heading">SECTION 6: Submitting Your Review</div>
                <div class="panel-body">

                    @using (Html.BeginForm("submitReview", "ReviewForm", FormMethod.Post))
                    {
                        @Html.HiddenFor(model => model.Id)
                        <div class="col-lg-10">
                            <textarea class="form-control" id="txtComment" name="comment" placeholder="Comment"></textarea>
                        </div>
                        <div class="col-lg-2">
                            <button type="submit" name="Command" value="reject" class="btn btn-danger">Reject</button>
                            <button type="submit" name="Command" value="accept" class="btn btn-primary">Accept</button>
                        </div>

                    }


                </div>
            </div>
        </div>

        </div>
    </div>
    @section scripts {
        <script src="~/Scripts/jquery-ui.min.js"></script>
        <script src="~/Scripts/jquery.signature.min.js"></script>
        <script src="~/Scripts/excanvas.js"></script>
        <script>
            $(document).ready(function () {
                $("#insertInCarceratedStep7").validate({
                    ignore: "",
                    rules: {
                        sign1: { required: true },
                        date1: {
                            required: true
                        },
                        sign2: { required: true },
                        date2: {
                            required: true
                        }
                    },
                    highlight: function (element) {
                        $(element).closest('.form-group').addClass('has-error');
                    },
                    unhighlight: function (element) {
                        $(element).closest('.form-group').removeClass('has-error');
                    }
                });
            });
            $('#sig1').signature({
                change: function (event, ui) {
                    $("#txtSignature1").val($('#sig1').signature('toJSON'));
                }
            });
            $('#clearButton1').click(function () {
                $('#sig1').signature('clear');
            });
            $('#sig2').signature({
                change: function (event, ui) {
                    $("#txtSignature2").val($('#sig2').signature('toJSON'));
                }
            });
            $('#clearButton2').click(function () {
                $('#sig2').signature('clear');
            });
            $("#txtDate,#txtDate2").datepicker();
            @if (Model != null && Model.type == 1 && Model.IncarceratedStep7 != null)
        {
            <text>
            var sign1 = @Model.IncarceratedStep7.sign1.Replace('"',' ')
                    $('#sig1').signature('draw', sign1);
            var sign2 = @Model.IncarceratedStep7.sign2.Replace('"', ' ')
            $('#sig2').signature('draw', sign2);
            </text>
        }
            @if (Model != null && Model.type == 1 && Model.NonIncarceratedStep7 != null) {
            <text>
            var sign1 = @Model.NonIncarceratedStep7.sign1.Replace('"',' ')
                    $('#sig1').signature('draw', sign1);
            var sign2 = @Model.NonIncarceratedStep7.sign2.Replace('"', ' ')
            $('#sig2').signature('draw', sign2);
            </text>
        }

        </script>
        @*$('#isEmptyButton').click(function() {
            alert('Is empty? ' + $('#whenChangedSignature').signature('isEmpty'));
            });*@
    }
